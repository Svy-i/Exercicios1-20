import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner entrada = new Scanner(System.in);
    float horas;
    float min;
    
    System.out.println("Quantas horas se passaram desde o ínicio do dia: ");
    horas = entrada.nextFloat();
    
    min = horas*60;
    System.out.println("Se passaram " + min + " desde o ínicio do dia");
  }
}
