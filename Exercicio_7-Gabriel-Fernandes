import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner entrada = new Scanner(System.in);
    int valor;
    int valorQuadrado;
    int valorCubo;
    
    System.out.println("Insira um número de 0 a 10: ");
    valor = entrada.nextInt();
    valorQuadrado = valor*valor;
    valorCubo = valor*valor*valor;
    Integer.toString(valorQuadrado);
    Integer.toString(valorCubo);
    System.out.printf("O quadrado desse número é: " + valorQuadrado + "\n");
    System.out.printf("O cubo desse número é: " + valorCubo + "\n");
    
  }
}
